      <div  class="text-center m-5">
        <h3 class="text-body-emphasis">طلب خدماتي</h3>
         
      </div>
      <form  action="/" method="post" name="user">
        <div class="mb-3">
          <label for="name" class="form-label">اسمك:</label>
          <input type="text" class="form-control" id="name" name="name"  placeholder="اكتب اسمك هنا:" required>
        </div>
        <div class="mb-3">
          <label for="phone" class="form-label">الرقم</label>
          <input type="number" class="form-control" id="phone" name="phone" placeholder="ادخل رقمك هنا:" required  min="700000000" max="789999999">
        </div>
        <div class="mb-3">
          <label for="typeorder" class="form-label">نوع الطلب</label>
          <select id="typeorder" class="form-select" name="typeorder">
            <option selected hidden>نوع الطلب</option>
            <option value="تصميم">تصميم</option>
            <option value="برمجة">برمجة</option>
            <option value="اخرى">اخرى</option>
          </select>
        </div>
        <div class="mb-3">
          <label for="order" class="form-label">الطلب</label>
          <textarea class="form-control" id="order" name="order" placeholder="ادخل طلبك هنا:" required></textarea>
        </div>
        <button type="submit" class="btn btn-primary">ارسل الطلب</button>
      </form>

    </div>
